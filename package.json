{
  "name": "FrontMan",
  "version": "0.1.1",
  "description": "Frontend project seed",
  "engines": {
    "node": ">=6.9.1"
  },
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "NODE_ENV=production node index.js",
    "start": "node index.js",
    "lint:js": "eslint *.js test src/js",
    "lint:css": "node stylelint.js",
    "lint": "npm run lint:js && npm run lint:css",
    "test": "mocha --compilers js:babel-register",
    "coverage": "istanbul cover _mocha -- --compilers js:babel-register",
    "validate": "html5validator --root build/",
    "verify": "npm run lint && npm run test && npm run coverage && npm run validate",
    "clean": "rm -rf build coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zharikovpro/frontman.git"
  },
  "author": "Andrey Zharikov & Nikolay Govorov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zharikovpro/frontman/issues"
  },
  "homepage": "https://github.com/zharikovpro/frontman#readme",
  "dependencies": {
    "autoprefixer": "^6.5.1",
    "babel": "^5.8.19",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.10.3",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.2.0",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "glob": "^7.1.1",
    "html-loader": "^0.4.3",
    "istanbul": "^1.1.0-alpha.1",
    "lost": "^7.0.3",
    "metalsmith": "^2.2.2",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-browser-sync": "^1.1.1",
    "metalsmith-ignore": "^0.1.2",
    "metalsmith-in-place": "^1.4.4",
    "metalsmith-layouts": "^1.6.5",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-permalinks": "^0.5.0",
    "metalsmith-prefixoid": "^1.0.3",
    "minimatch": "^3.0.3",
    "mocha": "^2.2.5",
    "mocha-loader": "^0.7.1",
    "ms-webpack": "^1.0.2",
    "postcss": "^5.2.5",
    "postcss-custom-properties": "^5.0.1",
    "postcss-for": "^2.1.1",
    "postcss-loader": "^1.1.0",
    "postcss-reporter": "^1.4.1",
    "promise": "^7.1.1",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.5.0",
    "stylelint-config-standard": "^14.0.0",
    "wave-pathfinder": "^0.1.0",
    "webpack": "^1.13.1"
  }
}
