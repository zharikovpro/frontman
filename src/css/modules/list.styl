.b-list
    display block
    margin-top 15px

    &__item
        list-style none
        font-family $font-main
        color #63717d
        font-size 16px
        font-weight 400
        line-height 22px

        &::before
            content ''
            display inline-block
            margin 0 11px 1px -20px
            border-radius 5px
            width 10px
            height 10px
            background-color #1dd384
